<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href=" /styles.css" />
    <title>Schedules</title>
</head>


<body>
    <%- include('../partials/header'); %>
        <main>
            <table>
                <thead>
                    <tr>
                        <th colspan="1">User Id</th>
                        <th colspan="1">Day</th>
                        <th colspan="1"> Start Time </th>
                        <th colspan="1"> End Time</th>

                    </tr>
                </thead>
                <tbody>


                    <tr colspan="2">
                        <% for ( i=0 ; i< user_schedule.length; i++){ %>
                            <td>

                                <%= user_schedule[i].user_id %>
                            </td>
                            <td>
                                <%= user_schedule[i].day %>
                            </td>
                            <td>
                                <%= user_schedule[i].start_at %>
                            </td>
                            <td>
                                <%= user_schedule[i].end_at %>
                            </td>
                    </tr>
                    <% } %>

                </tbody>
            </table>

        </main>
</body>

</html>