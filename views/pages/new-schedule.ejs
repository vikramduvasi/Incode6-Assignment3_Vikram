<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href=" /styles.css" />
    <title>New Schedule</title>
</head>

<body>

    <%- include('../partials/header'); %>

        <main>
            <form method="POST" action="/schedules" id="form">
                <div class="form-control">
                    <label for="user_id">User Id</label>
                    <input type="number" id="user_id" name="user_id">
                </div>
                <div class="form-control">
                    <label for="day">Day</label>
                    <input type="number" id="day" name="day">
                </div>
                <div class="form-control">
                    <label for="start_at">Start Time</label>
                    <input type="text" id="start_at" name="start_at">
                </div>
                <div class="form-control">
                    <label for="end_at">End Time</label>
                    <input type="text" id="end_at" name="end_at">
                </div>
                <button type="submit">Add New Schedule</button>
            </form>
        </main>

        <%- include('../partials/footer'); %>

</body>

</html>